          <p class="display-6 lead text-center">Here you can check on the status of your current listings and create new
            listings.</p>
        </ol>
        {{/with}}
      </div>
    </div>
  </div>
  <div class="jumbotron" id="jumboTwo">
  
  </div>
 
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
